pipeline {
    agent any
    stages {
        stage('Example') {
            steps {
                echo 'Hello World'
            }
        }
    }
    post {
        always {
            echo 'I will always say Hello again!'
            cleanWs()
        }
        changed{
            echo 'I will say Hello only when its changed'
        }
        failure{
            echo 'I will say Hello only when its Failure'
        }
        success{
            echo 'I will say Hello only when its success'
            cleanWs()
        }
    }
}